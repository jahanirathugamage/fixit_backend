{
  "crons": [
    {
      "path": "/api/cron/generate-recurring-jobs",
      "schedule": "0 2 * * *",
      "headers": {
        "Authorization": "Bearer ${CRON_SECRET}"
      }
    },
    {
      "path": "/api/cron/recurring-reminders",
      "schedule": "10 2 * * *",
      "headers": {
        "Authorization": "Bearer ${CRON_SECRET}"
      }
    }
  ]
}
